  },
  "keywords": [
    "vite-plugin",
    "mpa",
    "MPA",
    "multi-page",
    "html-template"
  ],
  "dependencies": {
    "ejs": "^3.1.8",
    "html-minifier-terser": "7.0.0",
    "shelljs": "0.8.5"
  },
  "devDependencies": {
    "unbuild": "2.0.0",
    "@types/ejs": "^3.1.1",
    "@types/html-minifier-terser": "^7.0.0",
